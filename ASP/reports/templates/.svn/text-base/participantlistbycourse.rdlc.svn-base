        <rd:TableName>participantlistbycourseandyear1</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT distinct top 4 s.studentid,s.firstname,s.lastname,s.title,s.address1,s.city,s.zipcode,s.org,
ct.countryname,c.courseid,c.crsetitle1,c.trainfrom,c.trainto
FROM courses c,student s,studentcourse sc,application a,country ct
WHERE 
sc.courseid=c.courseid AND s.countryid=ct.countryid AND
sc.studentid=s.studentid AND sc.courseid=@courseid AND a.year=@year
ORDER BY s.studentid ASC
</CommandText>
        <DataSourceName>usttiConnectionString</DataSourceName>
      </Query>
