      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT distinct top 4 s.studentid,s.firstname,s.lastname,s.title,s.address1,s.city,s.zipcode,s.org,
ct.countryname,c.courseid,c.crsetitle1,c.trainfrom,c.trainto
FROM courses c,student s,studentcourse sc,application a,country ct
WHERE 
sc.courseid=c.courseid AND s.countryid=ct.countryid AND
sc.studentid=s.studentid AND sc.courseid=@courseid AND 
a.year=@year AND s.studentid NOT IN 
(
       SELECT distinct top 4 s1.studentid
       FROM courses c1,student s1,studentcourse sc1,application a1,country ct1
       WHERE 
       sc1.courseid=c1.courseid AND s1.countryid=ct1.countryid AND
       sc1.studentid=s1.studentid AND sc1.courseid=@courseid AND  
       a1.year=@year ORDER BY s1.studentid ASC
)
ORDER BY s.studentid ASC
</CommandText>
        <DataSourceName>usttiConnectionString</DataSourceName>
      </Query>
