using System;
using System.Data;
using System.Configuration;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;
using System.Web.UI.HtmlControls;

namespace USTTI.Base
{

    public class Application
    {
        private Decimal _ApplicationID;
        private string _SID;
        private string _OID;
        private string _AppDate;
        private string _ArrUS;
        private string _DeptUS;
        private string _Year;
        private string _HotelDC;
        private string _ChkoutDate;
        private string _NeedsFund;
        private string _Memo;

        public Application()
        {

        }

        public Decimal ApplicationID
        {
            get
            {
                return _ApplicationID;
            }
            set
            {
                _ApplicationID = value;
            }
        }

        public string SID
        {
            get
            {
                return _SID;
            }
            set
            {
                _SID = value;
            }
        }

        public string OID
        {
            get
            {
                return _OID;
            }
            set
            {
                _OID = value;
            }
        }

        public string AppDate
        {
            get
            {
                return _AppDate;
            }
            set
            {
                _AppDate = value;
            }
        }

        public string ArrUS
        {
            get
            {
                return _ArrUS;
            }
            set
            {
                _ArrUS = value;
            }
        }

        public string DeptUS
        {
            get
            {
                return _DeptUS;
            }
            set
            {
                _DeptUS = value;
            }
        }

        public string Year
        {
            get
            {
                return _Year;
            }
            set
            {
                _Year = value;
            }
        }

        public string HotelDC
        {
            get
            {
                return _HotelDC;
            }
            set
            {
                _HotelDC = value;
            }
        }

        public string ChkoutDate
        {
            get
            {
                return _ChkoutDate;
            }
            set
            {
                _ChkoutDate = value;
            }
        }

        public string NeedsFund
        {
            get
            {
                return _NeedsFund;
            }
            set
            {
                _NeedsFund = value;
            }
        }

        public string Memo
        {
            get
            {
                return _Memo;
            }
            set
            {
                _Memo = value;
            }
        }
    }

}
