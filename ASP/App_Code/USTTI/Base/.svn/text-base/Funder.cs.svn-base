using System;
using System.Data;
using System.Configuration;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;
using System.Web.UI.HtmlControls;

namespace USTTI.Base
{

    public class Funder
    {
        private Decimal _FunderID;
        private int _FunderTypeID;
        private string _Description;
        public Funder()
        {

        }

        public Decimal FunderID
        {
            get
            {
                return _FunderID;
            }
            set
            {
                _FunderID = value;
            }
        }

        public int FunderTypeID
        {
            get
            {
                return _FunderTypeID;
            }
            set
            {
                _FunderTypeID = value;
            }
        }

        public string Description
        {
            get
            {
                return _Description;
            }
            set
            {
                _Description = value;
            }
        }
    }

}
